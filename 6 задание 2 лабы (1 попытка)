    unsigned long long int KAKMNOGOBYKV=a+b+c+d+e+f+g+h+i+j+k+l+m+n+o+p+q+r+s+t+u+v+w+x+y+z+aa+ab+ac+ad+ae+af+ag+ah+ai+aj+ak+al+am+an+ao+ap+aq+ar+as+at+au+av+aw+ax+ay+az+ba+bb+bc+bd+be+bf+bg+bh+bi+bj+bk+bm+bl+bn; //сумма всех чисел
    
    
    cout << KAKMNOGOBYKV; //вывод суммы
