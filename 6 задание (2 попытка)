#include <math.h>
#include <iostream>
#include <stdio.h>

using namespace std;

 
int
main ()
{
    unsigned long long int summa = 1, riceOnCurrentBlock = 1;
    for(int i = 1; i <= 64; i++) {
        riceOnCurrentBlock *= 2;
        summa += riceOnCurrentBlock; // sumRice = sumRice + riceOnCurrentBlock;
    }
    cout << "Partia gorditsa vamu,i vidavat vam " << summa << " zeren risa!";
     return 0;
}
