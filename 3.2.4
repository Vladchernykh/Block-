#include <iostream> //подключение библиотеки

using namespace std; //импорт всего пространства имен std


int a; //дано число а
int i = 1; //число i которое равно 1
int b = 1; //и число b которое тоже равно 1

main () //главная функция main ()
{

    cin >> a; //ввод числа a

    while(b < a)) //while (то есть цикл) будет работать пока b<a
    { 
        b = b * i; //умножение в цикле
        i++; //прибавление (i+1) каждый новый цикл
    }
    if(b == a) cout << "число является факториалом"; // == равно ли (сравнение),если b= a?после чего выводится если это так
    else cout << "число не является факториалом"; //если count не равно a,то выводится текст 
 
    return 0; //выход из функции с выводом
}
