#include <iostream> //подключение библиотеки


using namespace std;  //импорт всего пространства имен std

int main() //главная функция main ()


{
    int A; //даётся число А (которое по условию пятизначное)
    cin >> A; //вводим число а 
    int B = A / 100; //делим на 100 чтобы получить первые три числа (как например 10000-100=100)
    int C = A % 100; //делим на 100,но выводим остаток,тобишь последние 2 числа (10024/100=100,24 выводится число 24)
    int D = B / C; //первое число делится на второе по условию (которое будет целым)
    cout << D << endl ; //вывод деления (ответа)
    return 0; //выход из функции с выводом
}
